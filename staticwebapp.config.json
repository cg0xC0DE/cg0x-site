{
  "navigationFallback": {
    "rewrite": "/index.html",
    "exclude": ["/_next/*", "/images/*", "/*.ico", "/*.png", "/*.svg"]
  },
  "globalHeaders": {
    "X-Frame-Options": "DENY",
    "X-Content-Type-Options": "nosniff",
    "Referrer-Policy": "strict-origin-when-cross-origin"
  },
  "mimeTypes": {
    ".json": "application/json",
    ".js": "application/javascript",
    ".css": "text/css"
  }
}
